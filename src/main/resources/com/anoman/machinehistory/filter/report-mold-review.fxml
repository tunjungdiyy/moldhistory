            <Insets left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
         <content>
            <TableView fx:id="tableMold" prefHeight="200.0" prefWidth="200.0">
              <columns>
                <TableColumn fx:id="columnName" maxWidth="400.0" minWidth="200.0" prefWidth="300.0" text="MOLD" />
