            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
               <children>
                  <Label prefHeight="20.0" text="TABLE MOLD" textAlignment="CENTER" HBox.hgrow="ALWAYS">
